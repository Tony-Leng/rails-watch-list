<div class="container">
  <div class="d-flex justify-content-between align-items-center mt-3">
    <h1>Lists</h1>
    <%= link_to lists_path do %>
      <i class="fas fa-plus-square btn-light"></i>
    <% end %>
  </div>
  <div class="cards mb-5">
    <% @lists.each do |list| %>
      <%= list.name %>
    <% end %>
  </div>
</div>
